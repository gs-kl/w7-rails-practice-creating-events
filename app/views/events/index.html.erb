<table>
  <tr>
    <th>Date</th>
    <th>Description</th>
    <th>Requires ID?</th>
    <th>Capacity</th>
    <th>Directions</th>
  </tr>
<% @events.each do |event| %>
  <tr>
    <td><%= event.date %></td>
    <td><%= event.description %></td>
    <td><%= event.requires_id %></td>
    <td><%= event.capacity %></td>
    <td><%= event.directions %></td>
  </tr>
<% end %>
</table>
